    <style name="AppTheme2" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorbl</item>
        <item name="colorAccent">@color/colorAccent</item>


    </style>

    <style name="TextLabel2" parent="TextAppearance.AppCompat">

        <!-- Hint color and label color in FALSE state -->
        <item name="android:textColorHint">@color/colorbl</item>
        <item name="android:textSize">20sp</item>

        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->

        <item name="colorAccent">@color/colorbl</item>
        <item name="colorControlNormal">@color/colorbl</item>
        <item name="colorControlActivated">@color/colorbl</item>
    </style>


    <style name="TextLabel" parent="TextAppearance.AppCompat">

        <!-- Hint color and label color in FALSE state -->
        <item name="android:textColorHint">#fff</item>
        <item name="android:textSize">20sp</item>

        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->

        <item name="colorAccent">#fff</item>
        <item name="colorControlNormal">#fff</item>
        <item name="colorControlActivated">#fff</item>
    </style>


